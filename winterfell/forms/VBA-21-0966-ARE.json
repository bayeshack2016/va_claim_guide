    {
      "key": "veteran_home_apartment_number",
      "type": "input",
      "templateOptions": {
        "label": "Veteran Apartment Number",
        "placeholder": "Name"
      }
    },
    {
      "key": "veteran_home_city",
      "type": "input",
      "templateOptions": {
        "label": "Veteran City",
        "placeholder": ""
      }
    },
    {
      "key": "veteran_home_state",
      "type": "input",
      "templateOptions": {
        "label": "Veteran State",
        "placeholder": ""
      }
    },
    {
      "key": "veteran_home_zip_code",
