{
  "name": "INTENT TO FILE A CLAIM FOR COMPENSATION AND/OR PENSION, OR SURVIVORS PENSION AND/OR DIC",
  "description": "This Form Is Used to Notify VA of Your Intent to File for General Benefits",
  "fields": [
    {
      "key": "filing_for_self",
      "type": "radio",
      "templateOptions": {
        "label": "Are you filing for yourself, or for someone else?",
        "options": [
          {
            "name": "For myself",
            "value": true
          },
          {
            "name": "For someone else",
            "value": false
          }
        ]
      },
      "defaultValue": true
    },
    {
      "key": "represented_by_surviving_dependent_y_n",
      "type": "radio",
      "templateOptions": {
        "label": "Are you the dependent of a deceased veteran?",
        "options": [
          {
            "name": "Yes",
            "value": "Yes"
          },
          {
            "name": "No",
            "value": "No"
          }
        ]
      },
      "hideExpression": "model.filing_for_self"
    },
    {
      "key": "contact_email",
      "type": "input",
      "optionsTypes": [
        "email"
      ],
      "templateOptions": {
        "label": "Your Email",
      }
    },
    {
      "key": "veteran_sex",
      "type": "radio",
      "templateOptions": {
        "label": "Veteran's Gender",
        "options": [
          {
            "name": "Male",
            "value": "Male"
          },
          {
            "name": "Female",
            "value": "Female"
          }
        ]
      }
    },
    {
      "key": "veteran_intent_to_file_compensation_y_n",
      "type": "radio",
      "templateOptions": {
        "label": "Do you intend to file for compensation related benefits?",
        "options": [
          {
            "name": "Yes",
            "value": "Yes"
          },
          {
            "name": "No",
            "value": "No"
          }
        ]
      }
    },
    {
      "key": "veteran_intent_to_file_pension_y_n",
      "type": "radio",
      "templateOptions": {
        "label": "Do you intend to file for pension related benefits?",
        "options": [
          {
            "name": "Yes",
            "value": "Yes"
          },
          {
            "name": "No",
            "value": "No"
          }
        ]
      }
    },
    {
      "key": "veteran_previous_claim_with_va_y_n",
      "type": "radio",
      "templateOptions": {
        "label": "Have you, or the veteran you represent, ever previously filed a claim with the VA?",
        "options": [
          {
            "name": "Yes",
            "value": "Yes"
          },
          {
            "name": "No",
            "value": "No"
          }
        ]
      }
    },
    {
      "key": "veteran_attorney_or_vso",
      "type": "textarea",
      "templateOptions": {
        "label": "Name of attorney, agent, or veterans service organization",
        "rows": 2,
        "columns": 100
      }
    },
    {
      "key": "claimant_ssn",
      "type": "input",
      "templateOptions": {
        "label": "Claimant's social security number",
        "placeholder": "000-00-0000"
      },
      "optionsTypes": [
        "ssn"
      ],
      "hideExpression": "model.filing_for_self"
    },
    {
      "key": "veteran_ssn",
      "type": "input",
      "templateOptions": {
        "label": "Veteran's social security number",
        "placeholder": "000-00-0000"
      },
      "optionsTypes": [
        "ssn"
      ]
    },
    {
      "key": "veteran_first_name",
      "type": "input",
      "templateOptions": {
        "label": "First Name",
        "placeholder": "John"
      }
    },
    {
      "key": "veteran_middle_initial",
      "type": "input",
      "templateOptions": {
        "label": "Middle Initial",
        "placeholder": "C"
      }
    },
    {
      "key": "veteran_last_name",
      "type": "input",
      "templateOptions": {
        "label": "Last Name",
        "placeholder": "Doe"
      }
    },
    {
      "key": "claimant_first_name",
      "type": "input",
      "templateOptions": {
        "label": "Claimant's First Name",
        "placeholder": "John"
      },
      "hideExpression": "model.filing_for_self"
    },
    {
      "key": "claimant_middle_initial",
      "type": "input",
      "templateOptions": {
        "label": "Claimant's Middle Initial",
        "placeholder": "C"
      },
      "hideExpression": "model.filing_for_self"
    },
    {
      "key": "claimant_last_name",
      "type": "input",
      "templateOptions": {
        "label": "Claimant's Last Name",
        "placeholder": "Doe"
